<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="chatPane" prefHeight="470.0" prefWidth="870.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chat.client.templates.ChatViewController">
   <children>
      <TextField fx:id="fieldInput" layoutX="330.0" layoutY="379.0" onKeyPressed="#buttonPressed" prefHeight="26.0" prefWidth="430.0" />
      <Button layoutX="768.0" layoutY="379.0" mnemonicParsing="false" onAction="#sendMessage" prefHeight="26.0" prefWidth="80.0" text="Enter" />
      <TextArea fx:id="log" layoutX="330.0" layoutY="31.0" prefHeight="321.0" prefWidth="518.0" />
      <Button layoutX="17.0" layoutY="423.0" mnemonicParsing="false" onAction="#loadStartView" prefHeight="26.0" prefWidth="68.0" text="Back" />
   </children>
</AnchorPane>
